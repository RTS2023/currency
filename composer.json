{
    "name": "iulian/currency",
    "description": "A simple laravel package for a dynamically usage of currencies",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Currency\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "iulian",
            "email": "onofrei.iulian1@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^9.0",
        "illuminate/database": "^9.0",
        "spatie/laravel-translatable": "^6"
    },
    "extra": {
        "laravel": {
            "providers": ["\\Currency\\CurrencyServiceProvider"]
        }
    }
}
